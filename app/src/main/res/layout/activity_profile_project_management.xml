            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Error Project Name"
            android:textColor="@color/neutral100"
            android:textSize="@dimen/_14ssp"
            android:maxLines="2"
            android:ellipsize="end"
            android:fontFamily="@font/poppinssemibold"/>

        <TextView
            android:id="@+id/tv_projectCodeProfileProjectMgmnt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Error Kode Project"
            android:textColor="@color/grayTxt"
            android:textSize="@dimen/_11ssp"
            android:fontFamily="@font/poppinsregular"/>

        <TextView
            android:id="@+id/tv_branchNameProfileProjectMgmnt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Error Nama Cabang"
            android:textColor="@color/secondary_color"
            android:textSize="@dimen/_11ssp"
            android:fontFamily="@font/poppinsregular"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_4sdp"
            android:text="Alamat"
            android:textColor="@color/neutral100"
            android:textSize="@dimen/_12ssp"
            android:fontFamily="@font/poppinsmedium"/>

        <TextView
            android:id="@+id/tv_projectAddressProfileProjectMgmnt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Error Alamat"
            android:textColor="@color/neutral100"
            android:textSize="@dimen/_11ssp"
            android:fontFamily="@font/poppinsregular"/>

        <Space
            android:layout_width="wrap_content"
            android:layout_height="@dimen/_12sdp"/>

