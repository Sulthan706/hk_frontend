                android:layout_height="@dimen/_16sdp"
                android:src="@drawable/ic_chevron_right_secondary_color"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/ll_absentProfileProjectMgmntClient"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_clientProfileProjectMgmntClient"
