            android:layout_marginStart="@dimen/_8sdp"
            android:layout_marginEnd="@dimen/_4sdp"
            android:layout_marginVertical="@dimen/_6sdp"
            android:padding="@dimen/_4sdp"
            android:background="@drawable/bg_white_card"
            android:elevation="@dimen/_4sdp"
            app:layout_constraintTop_toBottomOf="@id/cl_operationalProfileProjectMgmnt"
            app:layout_constraintStart_toEndOf="@id/rl_ctalkProfileProjectMgmnt"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            >

            <RelativeLayout
                android:id="@+id/rl_clientProfileProjectMgmnt"
                android:layout_width="@dimen/_32sdp"
