            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_12sdp"
            android:layout_marginHorizontal="@dimen/_4sdp"
            android:layout_marginVertical="@dimen/_6sdp"
            android:padding="@dimen/_4sdp"
            android:visibility="gone"
            android:background="@drawable/bg_white_card"
            android:elevation="@dimen/_4sdp"
            android:layout_weight="1">

            <RelativeLayout
                android:id="@+id/rl_clientProfileProjectMgmntClient"
                android:layout_width="@dimen/_32sdp"
                android:layout_height="@dimen/_33sdp"
                android:background="@drawable/bg_disable"
                android:padding="@dimen/_4sdp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/ll_clientProfileProjectMgmntClient">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/immigration" />

            </RelativeLayout>

            <LinearLayout
                android:id="@+id/ll_clientProfileProjectMgmntClient"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginHorizontal="@dimen/_4sdp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/rl_clientProfileProjectMgmntClient"
                app:layout_constraintEnd_toStartOf="@id/iv_clientProfileProjectMgmntClient">

                <TextView
                    android:id="@+id/tv_clientProfileProjectMgmntClient"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textColor="@color/neutral100"
                    android:textSize="@dimen/_14ssp"
                    android:fontFamily="@font/poppinssemibold"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Client"
                    android:textColor="@color/grayTxt"
                    android:textSize="@dimen/_8ssp"
                    android:fontFamily="@font/poppinsmedium"/>

            </LinearLayout>

            <ImageView
                android:id="@+id/iv_clientProfileProjectMgmntClient"
                android:layout_width="@dimen/_16sdp"
                android:layout_height="@dimen/_16sdp"
                android:src="@drawable/ic_chevron_right_secondary_color"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/ll_clientProfileProjectMgmntClient"/>

        </androidx.constraintlayout.widget.ConstraintLayout>


    </LinearLayout>


